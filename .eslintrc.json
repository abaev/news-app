{
  "root": true,
  
  "env": {
    "node": true
  },
  
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  
  "parserOptions": {
    "parser": "babel-eslint"
  },
  
  "rules": {
  	"no-mixed-spaces-and-tabs": "off",
  	"no-unused-vars": "off",
  	"vue/no-unused-vars": "off"
  }
}